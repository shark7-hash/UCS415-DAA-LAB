#include <iostream>
using namespace std;

int main()
{
    int arr[] = {1, 5, 6, 10, 12};
    int n = 5;

    int ans = 1;

    for (int i = 0; i < n; i++)
    {
        int count = 1;
        for (int j = 0; j < n; j++)
        {
            if (i != j && abs(arr[i] - arr[j]) <= 10)
                count++;
        }
        ans = max(ans, count);
    }

    cout << "Maximum length of good subsequence = " << ans;
    return 0;
}
